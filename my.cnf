[server]

language=/opt/mariadb_server_10.5.12/share/english
datadir=/data
plugin-load=ha_mytile
plugin-load-add=ha_rocksdb
plugin_dir=/opt/mariadb_server_10.5.12/lib/plugin
plugin-maturity=experimental
# 32GB bufferpool
innodb_buffer_pool_size=34359738368
rocksdb-tmpdir=/data/tmp
tmpdir=/data/tmp
rocksdb-max-row-locks=1073741824
innodb_compression_algorithm=zlib